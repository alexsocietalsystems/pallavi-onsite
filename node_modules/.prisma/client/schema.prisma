datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

model Message {
  id        Int      @id @default(autoincrement())
  messageTemplate String
  timeUnit String
  countBefore Int
  createdAt DateTime @default(now())
}
